<h1>Create Song</h1>
<%= form_for @song do |f|%>
  <%= f.label :name, "Name" %><br>
  <%= f.text_field :name %><br><br>
  <%= f.label :artist_id, "Artist" %><br>
  <%= f.number_field  :artist_id %><br><br>
  <%= f.label :genre_id, "Genre" %>
  <%= f.number_field  :genre_id %> 
  <%= f.submit "Create Song" %>
<% end %>
